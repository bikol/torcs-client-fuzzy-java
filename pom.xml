    </repositories>
    <dependencies>
        <dependency>
            <groupId>net.sourceforge.jfuzzylogic</groupId>
            <artifactId>jFuzzyLogic</artifactId>
            <version>3.3</version>
            <type>jar</type>
        </dependency>
    </dependencies>
</project>